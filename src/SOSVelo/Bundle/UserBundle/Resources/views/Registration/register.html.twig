{% extends "SOSVeloTemplateBundle:Template:index.html.twig" %}
{% block body %}
    <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register" id="register">
        <h1>{{ 'layout.register'|trans({}, 'FOSUserBundle') }}</h1>
        <fieldset id="inputs">
            {{ form_widget(form) }}
        </fieldset>
        <fieldset id="actions">
            <input type="submit" value="{{ 'registration.submit'|trans({}, 'FOSUserBundle') }}" id="submit" />
        </fieldset>
    </form>
{% endblock %}